{
  "name": "Nhuly TRAN2LATED",
  "version": "1.0.0",
  "description": "Nhuly TRAN2LATED app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite--platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Nhuly TRAN2LATED\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Nhu Nguyen",
  "license": "MIT",
  "dependencies": {
    "electron-packager": "^15.5.1"
  },
  "devDependencies": {
    "electron": "^20.0.1"
  }
}
